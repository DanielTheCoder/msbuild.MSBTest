<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0"  >

  <PropertyGroup Condition="'$(TestrunnerImport)'==''">
    <!-- Import for Debug -->
    <TestrunnerImport Condition="'$(TestrunnerImport)'==''">$(MSBuildProjectDirectory)\..\testFramework\testrunner.targets</TestrunnerImport>
    <!-- Import for nuget -->
    <TestrunnerImport Condition="'$(TestrunnerImport)'==''">$(MSBuildThisFileDirectory)\testFramework\testrunner.targets</TestrunnerImport>
  </PropertyGroup>
  <Import Project="$(MSBuildProjectDirectory)\testFramework\testrunner.targets" />

  <Target Name="Build">
    <CallTarget Targets="RunTests" />
  </Target>

  <Target Name="Clean" >
    <CallTarget Targets="CleanTests" />
  </Target>
  
  <Target Name="Rebuild" >
    <CallTarget Targets="Clean; Build" />
  </Target>
  
  <Target Name="Publish">
    <CallTarget Targets="Build" />
  </Target>

  <Target Name="GetNativeManifest" />
  <Target Name="GetCopyToOutputDirectoryItems" />

  
  <Target Name="AfterBuild" AfterTargets="Build" />

</Project>
