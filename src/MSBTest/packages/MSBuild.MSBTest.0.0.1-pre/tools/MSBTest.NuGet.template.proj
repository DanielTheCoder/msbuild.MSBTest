<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  
  <PropertyGroup>
    <SC_NuGetTargetsEnabled Condition=" '$(SC_NuGetTargetsEnabled)'=='' ">true</SC_NuGetTargetsEnabled>
    <!-- Set the value of this property -->
    <_MSBTestTargetsNuGet>UPDATE-DURING-BUILD</_MSBTestTargetsNuGet>
    <MSBTestTargets Condition=" '$(MSBTestTargets)'=='' and Exists('$(_MSBTestTargetsNuGet)') ">$(_MSBTestTargetsNuGet)</MSBTestTargets>
  </PropertyGroup>
  
</Project>